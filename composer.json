{
    "name": "oscarricardosan/mapper",
    "description": "Paquete PHP que mapea los DocComments de la clase y permite definir mutators y accessors ",
    "license": "MIT",
    "authors": [
        {
            "name": "oscarricardosan",
            "email": "oscarricardosan@gmail.com"
        }
    ],
    "require": {
      "php" : ">=5.6.4"
    },
    "require-dev": {
      "phpunit/phpunit" : "~5.0"
    },
    "autoload": {
      "psr-4": {
        "Oscarricardosan\\Mapper\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Oscarricardosan\\Mapper\\Tests\\": "tests"
      }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
